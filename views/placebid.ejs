<!doctype html>
<html>
<head>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
    <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
    <script src="/js/dashboard.js"></script>
    <script src="/js/marketplace.js"></script>
    <script src="/js/logout.js"></script>
    <script src="/js/bid.js"></script>
    <style>
    .nav-bar{
      position: absolute;
      top:0;
      z-index: 1;
      background-color: #002540
    }
    .item{
      display: flex;
      justify-content: center;
      margin-top: 10px !important;
      margin-bottom: 10px;
    }
    .item-division{
      width:50%;
      display: flex;
      justify-content: center;
      align-items: center;
    }
    .item-division2{
      width:50%;
      display: flex;
      flex-direction: column;
    }
    .img-item{
      border-radius: 50%;
      width:100px;
      height: 100px;
    }
    .item-details{

    }
    .current{
      border-bottom-color: #F60100;
      font-size: 25px;
      border-bottom-width: 2px;
      border-bottom-style: solid;
      width: 300px;
      text-align: center;
    }
    </style>

</head>
<body>
  <nav class="nav-wrapper nav-bar">
    <a href="#" style="margin-left:20px" class="brand-logo"><%= name %></a>
      <ul id="nav-mobile" class="right hide-on-med-and-down">
        <li><a href="/dashboard">Dashboard</a></li>
        <li><a href="/">Newsfeed</a></li>
        <li><a href="/prediction">Prediction</a></li>
        <li><a href="/">Transactions</a></li>
        <li><a href="/marketplace">Marketplace</a></li>
        <li><button id="logout" class="waves-effect waves-light btn" id="logout" style="margin-right:10px;background-color:#F60100;">Logout</button></li>
      </ul>
  </nav>
  <br><br><br><br><br><br>
  <div class="container-fluid">
    <div class="row" style="display:flex;align-content:center;">
      <form id="place" class="col l5 m6 s12" style="margin:0 auto;">
        <div id="QNT" class="row" style="margin:0 auto;">
          <div class="input-field col s12 l6 m6">
            <input id="price" type="number" class="validate">
            <label for="price">Price</label>
          </div>
          <div class="input-field col s12 l6 m6">
            <input id="quantity" type="number" class="validate">
            <label for="quantity">Quantity</label>
          </div>
        </div>
        <input type="submit" value="Place your bid" class="col s12 waves-effect waves-light btn">
      </form>
    </div>
  </div>

</body>
</html>
